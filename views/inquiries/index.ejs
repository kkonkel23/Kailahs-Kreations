<%- include('../partials/header') %>

<h3>Make an Inquiry!</h3>

<form action="/nodemail" method="POST">
    <label>Email: </label>
    <input type="text" name="sender" placeholder="Your email here" required>
    <label>Name: </label>
    <input type="text" name="name" placeholder="Your name here">
    <label>Subject: </label>
    <input type="text" name="subject" placeholder="New Inquiry" required>
    <label>Message: </label>
    <input type="text" name="message" placeholder="Type your message..." required>
    <input type="submit" value="Send Email">
</form>
<a href="/nodemail">Send mail</a>


<%- include('../partials/footer') %>